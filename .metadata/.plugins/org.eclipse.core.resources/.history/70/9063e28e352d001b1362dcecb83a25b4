package review;

class 저장소{
	String data;
	
	String 다운로드() {
	data = "GTA5 게임";
	try {
		Thread.sleep(3000);  // 1000 ->1초
	} catch (InterruptedException e) {
		// TODO Auto-generated catch block
		System.out.println("괜찮아");
	}
	return data;
	}
}


public class Ex122 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		저장소 s = new 저장소();
		String game = s.다운로드();
		
		System.out.println(game+"실행");
	}

}
