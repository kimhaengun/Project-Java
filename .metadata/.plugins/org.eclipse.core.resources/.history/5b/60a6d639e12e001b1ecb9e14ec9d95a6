package ch13;

public class Thread01 {
public static void main(String[] args) {
	System.out.println("메인 스레드 실행됨");
	for (int i = 1; i < 11; i++) {
		try {
			System.out.println("메인 :"+i);
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
	System.out.println("메인 스레드 종료");
} //end of main
}
